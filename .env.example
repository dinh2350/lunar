# Lunar AI Agent Platform Configuration
# Copy to .env and fill in your values

# ── Server ──
NODE_ENV=development
LUNAR_PORT=3100
LUNAR_AGENT=main
LUNAR_MODEL=llama3.2:3b

# ── LLM Providers ──
OLLAMA_URL=http://localhost:11434
# GEMINI_API_KEY=your-gemini-api-key
# GROQ_API_KEY=your-groq-api-key

# ── Database ──
DATABASE_PATH=./data/lunar.db

# ── Channels (optional) ──
TELEGRAM_BOT_TOKEN=
# DISCORD_BOT_TOKEN=

# ── Eval ──
EVAL_URL=http://eval:8000

# ── Security (required in production) ──
# API_SECRET=your-secret-key-at-least-32-characters-long
RATE_LIMIT_RPM=60
CORS_ORIGINS=http://localhost:3000

# ── Monitoring ──
# ADMIN_CHAT_ID=your-telegram-chat-id-for-alerts
